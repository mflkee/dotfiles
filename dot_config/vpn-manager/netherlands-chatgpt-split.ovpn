config netherlands-kerkrade-s3.ovpn

pull-filter ignore "redirect-gateway"
pull-filter ignore "dhcp-option DNS"
route-nopull

script-security 2
setenv SPLIT_TUNNEL_STATE /run/vpn-manager/netherlands-chatgpt-split.routes
setenv SPLIT_TUNNEL_DOMAINS "api.openai.com platform.openai.com chat.openai.com chatgpt.com auth.openai.com status.openai.com labs.openai.com sora.openai.com codex.openai.com
codex-gateway.openai.com cli.openai.com files.openai.com"
route-up "/usr/local/lib/vpn-manager/chatgpt-split-routes.sh"
down "/usr/local/lib/vpn-manager/chatgpt-split-routes.sh"
